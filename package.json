  "devDependencies": {
    "microbundle": "^0.15.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  },
  "dependencies": {
  },
  "exports": {
    ".": {
       "import": "./dist/index.modern.js",
       "require": "./dist/index.js"
     }
   }
