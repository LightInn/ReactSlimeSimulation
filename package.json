{
  "name": "react-slime-simulation",
  "version": "1.0.0",
  "description": "A customizable React component for simulating slime mold behavior.",
  "author": "Breval Le Floch",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/LightInn/react-slime-simulation.git"
  },
  "keywords": [
    "react",
    "component",
    "simulation",
    "slime",
    "canvas",
    "generative"
  ],
  "main": "dist/index.js",
  "module": "dist/index.modern.js",
  "source": "src/index.js",
